{
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 5
    },
    "env": {
      "browser": true
    },
    "rules": {
      "func-names": [0],
      "function-paren-newline": [0],
      "no-useless-escape": [0],
      "no-var": [0],
      "prefer-arrow-callback": [0],
      "prefer-template": [0]
    }
}
